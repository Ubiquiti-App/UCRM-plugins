{
    "name": "mvqn/rest",
    "description": "A simple REST Client library for interfacing with generic REST Endpoints.",
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "require": {
        "php": "~7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "mvqn/annotations": "~1",
        "mvqn/collections": "~1",
        "mvqn/common": "~1",
        "mvqn/dynamics": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "7.2.7"
    },
    "autoload": {
        "psr-4": {
            "MVQN\\": "src/MVQN/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MVQN\\": "tests/MVQN/"
        }
    }
}
